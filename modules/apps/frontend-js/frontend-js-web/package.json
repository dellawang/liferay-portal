{
	"dependencies": {
		"clay": "2.17.0",
		"clay-alert": "2.17.0",
		"liferay-amd-loader": "4.1.0",
		"lodash.escape": "4.0.1",
		"lodash.groupby": "4.6.0",
		"lodash.isequal": "4.5.0",
		"lodash.memoize": "4.1.2",
		"lodash.unescape": "4.0.1",
		"metal": "2.16.8",
		"metal-anim": "2.0.1",
		"metal-component": "2.16.8",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-position": "2.1.2",
		"metal-soy": "2.16.8",
		"svg4everybody": "^2.1.8",
		"uuid": "^3.3.2"
	},
	"jest": {
		"setupFiles": [
			"<rootDir>/test/liferay/portlet/mock/setup.es.js",
			"<rootDir>/test/liferay/util/mock/setup.es.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/test/setup.es.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/test/setup.es.js",
			"<rootDir>/test/liferay/portlet/mock/portlet_data.es.js",
			"<rootDir>/test/liferay/portlet/mock/setup.es.js",
			"<rootDir>/test/liferay/util/mock/headers.es.js",
			"<rootDir>/test/liferay/util/mock/setup.es.js"
		],
		"testResultsProcessor": "liferay-jest-junit-reporter",
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"main": "index.es.js",
	"name": "frontend-js-web",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts check",
		"format": "liferay-npm-scripts fix",
		"test": "liferay-npm-scripts test",
		"watch": "liferay-npm-scripts webpack --watch"
	},
	"version": "4.0.0"
}
