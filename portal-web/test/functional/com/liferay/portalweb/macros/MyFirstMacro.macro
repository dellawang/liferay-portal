definition{

	macro assertLogin{
		Click(locator1 = "LRWelcome#AVATAR_BUTTON");

		AssertTextEquals.assertPartialText(
			locator1 = "LRWelcome#CURRENT_USER",
			value1 = "${assertUser}");
	}

		macro login{
		Open(locator1 = "http://localhost:8080");

		AssertClick(
			locator1 = "LRLogin#SIGN_IN_URL",
			value1 = "Sign In");

		Type(
			locator1 = "LRLogin#EMAIL_ADDRESS_FIELD",
			value1 = "${emailAddress}");
		Type(
			locator1 = "LRLogin#PASSWORD_FIELD",
			value1 = "${password}");

		var key_buttonName = "Sign In";

		AssertClick(
			locator1 = "LRLogin#BUTTON",
			value1 = "Sign In");

		var key_buttonName = "I Agree";

		if(IsElementPresent(locator1 = "LRLogin#BUTTON")){
			AssertClick(
				locator1 = "LRLogin#BUTTON",
				value1 = "I Agree");
		}

		var key_buttonName = "Save";

		if(IsElementPresent(locator1 = "LRPasswordReminder#PASSWORD_REMINDER_FIELD")){
			Type(
				locator1 = "LRPasswordReminder#PASSWORD_REMINDER_FIELD",
				value1 = "${midName}");

			AssertClick(
				locator1 = "LRLogin#BUTTON",
				value1 = "Save");
		}
	}

}