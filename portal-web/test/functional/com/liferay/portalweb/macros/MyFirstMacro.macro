definition{
	macro login{
		Open(locator1 = "http://localhost:8080");

		var key_buttonName = "${signInButton}";
		var key_buttonName = "${agreeButton}";
		var key_buttonName = "${saveButton}";

		AssertClick(
			locator1 = "LRLogin#SIGN_IN_URL",
			value1 = "${signINButton}");

		Type(
			locator1 = "LRLogin#EMAIL_ADDRESS_FIELD",
			value1 = "${emailAddress}");
		Type(
			locator1 = "LRLogin#PASSWORD_FIELD",
			value1 = "${password}");

		AssertClick(
			locator1 = "LRLogin#SIGN_IN_BUTTON",
			value1 = "${signINButton}");

		AsseryClick(
			locator1 = "LRTermsOfUse#AGREE_BUTTON",
			value1 = "${agreeButton}");

		Type(
			locator1 = "LRPasswordReminder#PASSWORD_REMINDER_FIELD",
			value1 = "ji");

		AssertClick(
			locator1 = "LRPasswordReminder#SAVE_BUTTON",
			value1 = "${saveButton}");
	}
	macro assertLogin{
		Click(locator1 = "LRWelcome#AVATAR_BUTTON");

		AssertTextEquals(
			locator1 = "LRWelcome#NAME_LI",
			value1 = "${assertUser}"]);
	}
}